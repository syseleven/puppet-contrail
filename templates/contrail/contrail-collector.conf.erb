# Collector configuration options
#

[DEFAULT]
cassandra_server_list = <%= @cassandra_server_list %>

# as per os-645, https://github.com/Juniper/contrail-controller/wiki/TTL-configuration-for-analytics-data
analytics_data_ttl = <%= @analytics_data_ttl %>
analytics_config_audit_ttl = <%= @analytics_config_audit_ttl %>
analytics_statistics_ttl = <%= @analytics_statistics_ttl %>
analytics_flow_ttl = <%= @analytics_flow_ttl %>

# dup = <%= @dup %>
# hostip = <%= @hostip %>
# hostname = <%= @hostname %>
# http_server_port = <%= @http_server_port %>
# log_category = <%= @log_category %>
# log_disable = <%= @log_disable %>
log_file = <%= @log_file %>
#log_files_count = <%= @log_files_count %>
#log_file_size = <%= @log_file_size %>
#log_file_size = <%= @log_file_size %>
log_level = <%= @log_level %>
#syslog_facility = LOG_USER
# log_local = <%= @log_local %>
# syslog_port = <%= @syslog_port %>
# test_mode = <%= @test_mode %>

[COLLECTOR]
port = <%= @collector_listen_port %>
server = <%= @collector_listen_ip %>

[DISCOVERY]
port = <%= @disc_server_port %>
server = <%= @disc_server_ip %>


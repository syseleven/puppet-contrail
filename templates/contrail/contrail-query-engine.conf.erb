# max_slice = <%= @max_slice %>
# max_tasks = <%= @max_tasks %>
# start_time = <%= @start_time %>
# test_mode = <%= @test_mode %>
syslog_facility = LOG_USER

[DISCOVERY]
port = <%= @disc_server_port %>
server = <%= @disc_server_ip %>

[REDIS]
port = <%= @redis_port %>
server = <%= @redis_server %>
